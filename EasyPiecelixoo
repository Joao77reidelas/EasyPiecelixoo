local Tab = Window:MakeTab({
    Name = "Others",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "Others"
})

Tab:AddButton({
	Name = "Go to sea 2(No need for level 245, Level 1 can go)",
	Callback = function()
local args = {
    [1] = "Purchase",
    [2] = "Sea2"
}

game:GetService("ReplicatedStorage").Shop_Remote:InvokeServer(unpack(args))

   		
  	end    
})

Tab:AddButton({
	Name = "Go to Sea 1",
	Callback = function()
local args = {
    [1] = "Purchase",
    [2] = "Sea1"
}

game:GetService("ReplicatedStorage").Shop_Remote:InvokeServer(unpack(args))

   		
  	end    
})

Tab:AddButton({
	Name = "lvl Max and inf money(Click here first and need lvl 50)",
	Callback = function()
while true do
wait(0.001)
local args = {
    [1] = "AcceptQuest",
    [2] = "Message1"
}

game:GetService("ReplicatedStorage").Quest_Remote:FireServer(unpack(args))
end

   		
  	end    
})

Tab:AddButton({
	Name = "lvl Max and inf money(Click here second and need lvl 50)",
	Callback = function()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

humanoidRootPart.CFrame = CFrame.new(-769, 14, -1839)			
  	end    
})

