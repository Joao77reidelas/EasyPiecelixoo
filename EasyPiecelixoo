local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({
    Name = "easy piece",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "N3nhum"
})

local Tab = Window:MakeTab({
    Name = "Kill Auras",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "kill auras"
})

local isToggleActive = false

Tab:AddToggle({
    Name = "Combat Kill aura‚úäüèª",
    Default = false,
    Callback = function(Value)
        isToggleActive = Value
        if isToggleActive then
            while isToggleActive do
                wait(0.0001)
                local co = coroutine.create(function()
                    local args = {
                        [1] = "Combat1"
                    }
                    game:GetService("Players").LocalPlayer.Character.Combat.Client.Event:FireServer(unpack(args))
                end)
                coroutine.resume(co)
            end
        end
    end
})

Tab:AddToggle({
    Name = "Metal Leg Kill auraü¶µüèªüî•",
    Default = false,
    Callback = function(Value)
        isToggleActive = Value
        if isToggleActive then
            while isToggleActive do
                wait(0.0001)
                local co = coroutine.create(function()
                    local args = {
    [1] = "MetalLeg1"
}

game:GetService("Players").LocalPlayer.Character:FindFirstChild("Metal Leg").ClientMetal.Event:FireServer(unpack(args))

                end)
                coroutine.resume(co)
            end
        end
    end
})

Tab:AddToggle({
    Name = "Karate kill auraü•ãüíß",
    Default = false,
    Callback = function(Value)
        isToggleActive = Value
        if isToggleActive then
            while isToggleActive do
                wait(0.0001)
                local co = coroutine.create(function()
                   local args = {
    [1] = "waterpunch"
}

game:GetService("Players").LocalPlayer.Character:FindFirstChild("Fish Karate").ClientFish.Event:FireServer(unpack(args))
 

                end)
                coroutine.resume(co)
            end
        end
    end
})

Tab:AddToggle({
    Name = "Dragonüî•üêâ",
    Default = false,
    Callback = function(Value)
        isToggleActive = Value
        if isToggleActive then
            while isToggleActive do
                wait(0.0001)
                local co = coroutine.create(function()
                    local args = {
    [1] = "waterflow1"
}

game:GetService("Players").LocalPlayer.Character:FindFirstChild("Dragon Combat").ClientDragon.Event:FireServer(unpack(args))


                end)
                coroutine.resume(co)
            end
        end
    end
})

Tab:AddToggle({
    Name = "metal legV2üåã(SUPER Lag use metal legV1)",
    Default = false,
    Callback = function(Value)
        isToggleActive = Value
        if isToggleActive then
            while isToggleActive do
                wait(0.0001)
                local co = coroutine.create(function()
                    local args = {
    [1] = "MetalLeg1",
    [2] = true
}

game:GetService("Players").LocalPlayer.Character:FindFirstChild("Fire Leg").ClientFireLeg.Event:FireServer(unpack(args))


                end)
                coroutine.resume(co)
            end
        end
    end
})

